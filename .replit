run = "python main.py"
entrypoint = "main.py"
modules = ["python-base-3.13", "postgresql-16"]

[nix]
channel = "unstable"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8501
externalPort = 80

[deployment]
deploymentTarget = "cloudrun"

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 36593104
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
